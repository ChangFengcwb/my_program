<?xml version="1.0" ?>
<!DOCTYPE hibernate-mapping PUBLIC
		"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.cui.po">
	<class name="Board" table="board">
		<id name="id" column="id" type="java.lang.Integer">
			<generator class="increment"/>
		</id>
		<property name="name" type="java.lang.String" column="name" not-null="true"/>
		<property name="description" type="java.lang.String" column="description"/>
		<property name="parentId" type="java.lang.Integer" column="parentId"/>
		<property name="boardImg" type="java.lang.String" column="boardImg"/>
		<many-to-one name="aid" column="aid" not-null="true"/>
	</class>
</hibernate-mapping>